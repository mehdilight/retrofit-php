{
    "name": "phpmystic/retrofit-php",
    "description": "A type-safe HTTP client for PHP inspired by Square's Retrofit. Turn your HTTP API into a PHP interface using attributes.",
    "version": "1.5.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "retrofit",
        "http",
        "client",
        "api",
        "rest",
        "http-client",
        "psr-7",
        "psr-18",
        "guzzle",
        "async",
        "retry",
        "cache",
        "interceptor",
        "type-safe",
        "streaming",
        "file-upload",
        "multipart",
        "xml",
        "serializer",
        "symfony"
    ],
    "autoload": {
        "psr-4": {
            "Phpmystic\\RetrofitPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpmystic\\RetrofitPhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "elmehdi elfatihi",
            "email": "elmehdielfatihi1111@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^7.3",
        "nyholm/psr7": "^1.8",
        "symfony/serializer": "^6.0|^7.0",
        "symfony/property-access": "^6.0|^7.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
