{
    "name": "phpmystic/retrofit-php",
    "description": "A type-safe HTTP client for PHP inspired by Square's Retrofit. Turn your HTTP API into a PHP interface using attributes.",
    "version": "1.2.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Phpmystic\\RetrofitPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpmystic\\RetrofitPhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "elmehdi elfatihi",
            "email": "elmehdielfatihi1111@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^7.3"
    },
    "scripts": {
        "test": "phpunit"
    }
}
